function CustomPageWindow(tabbed_window,show_navbar,title,pagecontent){var win=Ti.UI.createWindow({title:title,backgroundColor:'black'});var osname=Ti.Platform.osname,version=Ti.Platform.version,height=Ti.Platform.displayCaps.platformHeight,width=Ti.Platform.displayCaps.platformWidth;var isTablet=osname==='ipad'||(osname==='android'&&(width>899||height>899));var adgap=(isTablet?90:50);var adwidth=(isTablet?728:320);var ad_role=Ti.App.Properties.getInt('cloud_userrole',0)
var view_header_gap=0
if((ad_role<5)&&(Ti.Platform.osname!='mobileweb')){view_header_gap=adgap;var Admob=require('ti.admob');var ad;win.add(ad=Admob.createView({top:0,width:adwidth,height:adgap,publisherId:'a14fc70a8d46176',adBackgroundColor:'black',gender:'male',keywords:'movie'}));Ti.Geolocation.accuracy=Ti.Geolocation.ACCURACY_BEST;Ti.Geolocation.distanceFilter=0;Ti.Geolocation.purpose='To show you local ads, of course!';Ti.Geolocation.getCurrentPosition(function reportPosition(e){if(!e.success||e.error){}
else{win.add(Admob.createView({bottom:0,width:adwidth,height:adgap,publisherId:'a14fc70a8d46176',adBackgroundColor:'black',gender:'female',keywords:'shopping',location:e.coords}));}});}
var pagehtml='<html><head></head><body>'+pagecontent+'</body></html>'
var webview=Ti.UI.createWebView({top:view_header_gap,bottom:view_header_gap,html:pagehtml})
win.add(webview);if(tabbed_window&&show_navbar){win.navBarHidden=false;}else{win.navBarHidden=true;}
win.addEventListener('open',function(){logging('HTMLPAGE',title)});return win};module.exports=CustomPageWindow;